<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Members</title>
</head>
<body>
    <h1>Members</h1>
    <table>
        <tr>
            <th>Name</th>
            <th>Company</th>
            <th>Share Percentage</th>
        </tr>
        <% members.forEach(member => { %>
        <tr>
            <td><%= member.name %></td>
            <td><%= member.company ? member.company.name : 'N/A' %></td>
            <td><%= member.sharePercentage %></td>
        </tr>
        <% }); %>
    </table>
    <a href="/">Home</a>
</body>
</html>
